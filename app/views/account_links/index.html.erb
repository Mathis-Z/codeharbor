<p id="notice"><%= notice %></p>

<h1>Listing Account Links</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_links.each do |account_link| %>
      <tr>
        <td><%= link_to 'Show', account_link %></td>
        <td><%= link_to 'Edit', edit_account_link_path(account_link) %></td>
        <td><%= link_to 'Destroy', account_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account link', new_account_link_path %>
