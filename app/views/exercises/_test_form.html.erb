<div class="field">
  <%= f.label :content %><br>
  <div class="edit">
    <div class="editor"><%= test.content %></div>
    <textarea class="hidden" name="<%= test.id %>[content]" style="display:none;">
      <%= test.content %>
    </textarea>
  </div>
</div>
<div class="field">
  <%= f.label :feedback_message %><br>
  <%= text_field_tag test.id.to_s+'[feedback_message]', test.feedback_message %>
</div>
<div class="field">
  <%= f.label :testing_framework %><br>
  <%= collection_select test.id.to_s+'[testing_framework]', test.id, TestingFramework.all, :id, :name %>
</div>



